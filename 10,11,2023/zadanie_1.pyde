q=300
w=300

def setup():
    size(600, 600)  
    strokeWeight(10)
    point(300,300)

    
 
def draw():
    global q,w
    stroke(random(000,225)
           
    line(300,300,601,601)
    line(300,300,601,1)
    line(300,300,1,601)
    line(300,300,1,1)  
    q=q+2  
    w=w+2    
